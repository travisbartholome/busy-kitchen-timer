{
  "extends": ["airbnb"],
  "rules": {
    "comma-dangle": ["warn", "never"],
    "linebreak-style": "off",
    "no-console": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-one-expression-per-line": "off"
  },
  "env": {
    "browser": true,
    "node": true
  }
}